(err.response.data.message)\n            })\n        getIncomes()\n    }\n\n    const getIncomes = async () => {\n        const response = await axios.get(`${BASE_URL}get-incomes`)\n        setIncomes(response.data)\n        console.log(response.data)\n    }\n\n    const deleteIncome = async (id) => {\n        const res  = await axios.delete(`${BASE_URL}delete-income/${id}`)\n        getIncomes()\n    }\n\n    const totalIncome = () => {\n        let totalIncome = 0;\n        incomes.forEach((income) =>{\n            totalIncome = totalIncome + income.amount\n        })\n\n        return totalIncome;\n    }\n\n\n    //calculate incomes\n    const addExpense = async (income) => {\n        const response = await axios.post(`${BASE_URL}add-expense`, income)\n            .catch((err) =>{\n                setError(err.response.data.message)\n            })\n        getExpenses()\n    }\n\n    const getExpenses = async () => {\n        const response = await axios.get(`${BASE_URL}get-expenses`)\n        setExpenses(response.data)\n        console.log(response.data)\n    }\n\n    const deleteExpense = async (id) => {\n        const res  = await axios.delete(`${BASE_URL}delete-expense/${id}`)\n        getExpenses()\n    }\n\n    const totalExpenses = () => {\n        let totalIncome = 0;\n        expenses.forEach((income) =>{\n            totalIncome = totalIncome + income.amount\n        })\n\n        return totalIncome;\n    }\n\n\n    const totalBalance = () => {\n        return totalIncome() - totalExpenses()\n    }\n\n    const transactionHistory = () => {\n        const history = [...incomes, ...expenses]\n        history.sort((a, b) => {\n            return new Date(b.createdAt) - new Date(a.createdAt)\n        })\n\n        return history.slice(0, 3)\n    }\n\n\n    return (\n        <GlobalContext.Provider value={{\n            addIncome,\n            getIncomes,\n            incomes,\n            deleteIncome,\n            expenses,\n            totalIncome,\n            addExpense,\n            getExpenses,\n            deleteExpense,\n            totalExpenses,\n            totalBalance,\n            transactionHistory,\n            error,\n            setError\n        }}>\n            {children}\n        </GlobalContext.Provider>\n    )\n}\n\nexport const useGlobalContext = () =>{\n    return useContext(GlobalContext)\n}","/Users/macintoshhd/Downloads/expense-tracker-main/frontend/src/LogoutConfirmationModal.js",[],[],"/Users/macintoshhd/Downloads/expense-tracker-main/frontend/src/Loading.js",[],[],"/Users/macintoshhd/Downloads/expense-tracker-main/frontend/src/api.js",[],[],"/Users/macintoshhd/Downloads/expense-tracker-main/frontend/src/Components/Transaction/transaction.js",[],[],"/Users/macintoshhd/Downloads/expense-tracker-main/frontend/src/Components/Transaction/Transaction.js",[],[],{"ruleId":"181","severity":1,"message":"182","line":9,"column":10,"nodeType":"183","messageId":"184","endLine":9,"endColumn":14},{"ruleId":"181","severity":1,"message":"185","line":9,"column":15,"nodeType":"183","messageId":"184","endLine":9,"endColumn":26},{"ruleId":"186","severity":1,"message":"187","line":20,"column":8,"nodeType":"188","endLine":20,"endColumn":10,"suggestions":"189"},{"ruleId":"181","severity":1,"message":"190","line":9,"column":12,"nodeType":"183","messageId":"184","endLine":9,"endColumn":21},{"ruleId":"186","severity":1,"message":"191","line":13,"column":8,"nodeType":"188","endLine":13,"endColumn":10,"suggestions":"192"},{"ruleId":"181","severity":1,"message":"193","line":55,"column":11,"nodeType":"183","messageId":"184","endLine":55,"endColumn":19},{"ruleId":"181","severity":1,"message":"194","line":5,"column":8,"nodeType":"183","messageId":"184","endLine":5,"endColumn":12},{"ruleId":"181","severity":1,"message":"190","line":10,"column":12,"nodeType":"183","messageId":"184","endLine":10,"endColumn":21},{"ruleId":"186","severity":1,"message":"195","line":14,"column":8,"nodeType":"188","endLine":14,"endColumn":10,"s